{
  "name": "dyte-docs",
  "version": "1.20.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "docusaurus": "docusaurus",
    "start": "nodemon --exec \"npm run dev\"",
    "dev": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier -w src docs",
    "format:docs": "prettier -w docs",
    "typecheck": "tsc",
    "spell-check": "npx cspell docs/**/*.mdx"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.19.10",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@docsly/react": "^1.9.1",
    "@docusaurus/core": "^3.6.3",
    "@docusaurus/module-type-aliases": "^3.6.3",
    "@docusaurus/plugin-client-redirects": "^3.6.3",
    "@docusaurus/preset-classic": "^3.6.3",
    "@docusaurus/remark-plugin-npm2yarn": "^3.6.3",
    "@docusaurus/theme-live-codeblock": "^3.6.3",
    "@docusaurus/theme-mermaid": "^3.6.3",
    "@dytesdk/docs-ai-react": "^2.0.1",
    "@dytesdk/web-application": "^1.66.0",
    "@dytesdk/react-web-core": "file:modules/dytesdk-react-web-core-1.36.4-stripped.1.tgz",
    "@dytesdk/web-core": "file:modules/dytesdk-web-core-1.31.0-stripped.1.tgz",
    "@fluentui/react-icons": "^2.0.267",
    "@headlessui/react": "^2.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@stoplight/elements": "^8.5.1",
    "clsx": "^2.1.1",
    "nanoid": "^5.0.9",
    "param-case": "^3.0.4",
    "patch-package": "^8.0.0",
    "prism-react-renderer": "^2.4.0",
    "raw-loader": "^4.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-feather": "^2.0.10",
    "reactflow": "^11.11.4",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@docusaurus/tsconfig": "^3.6.3",
    "@styled-icons/boxicons-logos": "^10.47.0",
    "@swc/core": "^1.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "docusaurus-plugin-typedoc": "^1.1.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.1.7",
    "pascal-case": "^3.1.2",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "process": "^0.11.10",
    "react-markdown": "^9.0.1",
    "stream-browserify": "^3.0.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.16",
    "typedoc": "^0.27.4",
    "typedoc-plugin-markdown": "^4.3.2",
    "typescript": "^5.7.2",
    "url": "^0.11.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
